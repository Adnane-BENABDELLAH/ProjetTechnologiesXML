<?xml version="1.0" encoding="UTF-8"?>
<xs:schema id="Notes" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"> 
    
    <xs:element name="Notes">
        <xs:complexType>
            <xs:sequence>
                <xs:element  minOccurs="1" maxOccurs="15" ref="Note" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="Note">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="CNE" />
                <xs:element ref="FirstName" />
                <xs:element ref="LastName" />
                <xs:element ref="ModuleName" />
                <xs:element ref="NoteAvantRatt" />
                <xs:element ref="NoteApresRatt" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="CNE" type="xs:string" />
    
    <xs:element name="FirstName" type="xs:string" />
    
    <xs:element name="LastName" type="xs:string" />
    
    <xs:element name="ModuleName" type="xs:string" />
    
    <xs:element name="NoteAvantRatt" type="xs:decimal" />
    
    <xs:element name="NoteApresRatt" type="xs:decimal" />
    
</xs:schema>

